{
  "methods": {
    "private/get_affiliate_program_info": {
      "response": {
        "objectParams": {
          "link": {
            "description": "Affiliate link"
          }
        }
      }
    },
    "^private/get_settlement_history_by_(instrument|currency)$": {
      "response": {
        "objectParams": {
          "settlements": {
            "objectParams": {
              "session_bankruptcy": {
